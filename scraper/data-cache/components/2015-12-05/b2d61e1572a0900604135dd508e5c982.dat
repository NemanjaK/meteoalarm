	function selektfilterstanice () {
		
		var odabrana=0;
		
		//DA LI JE ODABRANA IJEDNA STANICA
		$(".stanicecheck:checked").each(function() {
			odabrana=1;
		});
		
		//PRODJI KROZ KOMPONENTE I UKLJUCI SVE AKO NIJE ODABRANA NI JEDNA STANICA
		if (odabrana==0) {
			
			$(".komponentecheck").each(function() {
				$(this).attr('disabled', false).show();
				$(this).closest('.komponentelabela').find('.naziv').show();
			});
			
			} else {
			$(".komponentecheck").each(function() {
				$(this).attr('disabled', true).hide();
				$(this).closest('.komponentelabela').find('.naziv').hide();
				 //$(this).closest('.naziv').hide();
			});
		}
		
		//PRODJI KROZ STANICE 
		$(".stanicecheck:checked").each(function() {
			
			//alert (String($(this).data('id')).indexOf(","));
			
			if (String($(this).data('id')).indexOf(",") > 0) {
				//alert ("vise komponenti");
				var nizkomponenti = $(this).data('id').split(",");
				$.each(nizkomponenti,function(i){
					
					//	$('.komponentecheck').children('option[value="' + nizkomponenti[i] + '"]').attr('disabled', false);
					
					$(".komponentecheck").each(function() {
						
						if (nizkomponenti[i]==$(this).val()) {
							//alert ($(this).val());
							$(this).attr('disabled', false).show();
							$(this).closest('.komponentelabela').find('.naziv').show();
						}
					});
					
				});
				
				} else {
				//alert ("jedna");
				var nizkomponenti = $(this).data('id');
				$(".komponentecheck").each(function() {
					
					if (nizkomponenti==$(this).val()) {
						//alert ($(this).val());
						$(this).attr('disabled', false).show();
						$(this).closest('.komponentelabela').find('.naziv').show();
					}
				});
				
			}
			
			
			
			
			
		});
		
		
	}
	
